{
  "name": "docdown-editor",
  "productName": "DocDown",
  "version": "0.2.2",
  "description": "DocDown Editor",
  "main": "main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 jest",
    "test-watch": "npm test -- --watch",
    "lint": "eslint --cache --ignore-path .gitignore --format=node_modules/eslint-formatter-pretty app test *.js",
    "lint-fix": "npm run lint -- --fix",
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=2096 -r babel-register server.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "flow-typed-install": "flow-typed install",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./app/main.development",
    "postinstall": "concurrently \"install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
    "dev": "npm run hot-server -- --start-hot",
    "package": "npm run build && build --publish never",
    "package-win": "npm run build && build --win --x64",
    "package-linux": "npm run build && build --linux",
    "package-all": "npm run build && build -mwl",
    "cleanup": "mop -v"
  },
  "build": {
    "productName": "DocDown",
    "appId": "org.livio.DocDown",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "replacer.html",
      "main.js",
      "main.js.map",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.tools"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    }
  },
  "directories": {
    "buildResources": "resources",
    "output": "release"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/livio/docdown-editor.git"
  },
  "author": "Daniel Farrell <daniel@mobelux.com>",
  "contributors": [
    "Liz Swain <liz@mobelux.com>",
    "Christian Bryant <christian@mobelux.como>"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/livio/docdown-editor/issues"
  },
  "keywords": [
    "markdown",
    "docdown",
    "editor",
    "smartdevicelink",
    "livio"
  ],
  "homepage": "https://github.com/livio/docdown-editor#readme",
  "devDependencies": {
    "asar": "^0.13.0",
    "aws-sdk": "^2.7.21",
    "babel-core": "^6.21.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-tcomb": "^0.3.24",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.1.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "babili-webpack-plugin": "^0.1.2",
    "boiler-room-custodian": "^0.6.2",
    "concurrently": "^3.1.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "devtron": "^1.4.0",
    "electron": "^1.4.13",
    "electron-builder": "^19.43.0",
    "electron-devtools-installer": "^2.0.1",
    "enzyme": "^3.1.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-compat": "^2.0.1",
    "eslint-plugin-flowtype": "^2.29.2",
    "eslint-plugin-flowtype-errors": "^3.3.5",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fbjs-scripts": "^0.8.1",
    "file-loader": "^1.1.5",
    "flow-bin": "^0.57.3",
    "flow-typed": "^2.0.0",
    "html-webpack-plugin": "^2.24.1",
    "jest": "^21.2.1",
    "jest-immutable-matchers": "^1.4.0",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "node-sass": "^4.0.0",
    "react-addons-test-utils": "^15.4.1",
    "react-test-renderer": "^16.0.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.1",
    "sass-loader": "^6.0.6",
    "spectron": "^3.4.1",
    "style-loader": "^0.19.0",
    "tachyons-sass": "^4.5.6",
    "tcomb": "^3.2.16",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.14.0",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "csv-parse": "^2.0.0",
    "draft-js": "^0.10.4",
    "draft-js-code": "^0.3.0",
    "electron-debug": "^1.1.0",
    "font-awesome": "^4.7.0",
    "highlight.js": "^9.8.0",
    "immutable": "^3.8.1",
    "mustache": "^2.3.0",
    "prismjs": "^1.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^5.0.1",
    "react-split-pane": "^0.1.52",
    "recompose": "^0.26.0",
    "redux": "^3.6.0",
    "redux-actions": "^2.2.1",
    "redux-observable": "^0.16.0",
    "redux-persist": "^5.1.0",
    "redux-persist-transform-immutable": "^4.1.0",
    "redux-thunk": "^2.1.0",
    "remarkable": "^1.6.2",
    "remarkable-docdown": "^1.0.0",
    "reselect": "^3.0.1",
    "rxjs": "^5.0.2",
    "source-map-support": "^0.5.0",
    "uuid": "^3.0.1",
    "watch-rx": "^1.0.0"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  }
}
